package constructor;

public class House {
	public String caption;
	public double price;
	public double m2;
	public int floorCount;
	public boolean pool;
	public House() {
		
	}
	public House(String caption,double price,double mt2,int floorCount,boolean pool) {
		this.caption=caption;
		this.price=price;
		this.mt2=mt2;
		this.floorCount=floorCount;
		this.pool=pool;
	}
}
