package arabamCom;

public class Otomobil {
	public String name;
	public String brand;
	public String model;
	public double km;
	public String color;
	public boolean lpg;
	public boolean fuelOil;
	public boolean diesel;
	public double price;

}

public void print() {
	System.out.print("Ýsim : ");
	System.out.println(this.name);   
	System.out.print("      Fiyat :" + this.price);
	System.out.println("      Marka :" + this.brand);
	System.out.println("Renk :" + this.color);
	System.out.println("Yakýt Türü :" + this.fuelOil);
}
