
public class Product {

	public String name;
	public double cost;
	public double quan;
	public double price;
	public void print() {
		System.out.print("Meyve : ");
		System.out.println(this.name);   
		System.out.print("Fiyat :" + this.cost);
		System.out.println("Kilo :" + this.quan);
		System.out.println("Toplam Fiyat :" + this.getPrice());
	}

	public double getPrice() {
		return this.cost * this.quan;
	}
}
