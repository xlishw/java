
public class produck {
		public String name;
		public double cost;
		public double quan;
		public double price;

		public void print() {
			System.out.print("ürün : ");
			System.out.println(this.name);   
			System.out.print("      Fiyat :" + this.cost);
			System.out.println("      Miktar :" + this.quan);
			System.out.println("Toplam Fiyat :" + this.getPrice());
		}

		public double getPrice() {
			return this.cost * this.quan;

}
